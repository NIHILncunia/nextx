import React from 'react';
import type { Metadata, ResolvingMetadata } from 'next';

import { <%- pascalCaseName %> } from '<%- componentImportPath %>';

interface Props {
  params: Promise<{ <%- dynamicParam %>: string }>;
  searchParams: Promise<{ [key: string]: string | string[] | undefined }>;
}

export async function generateMetadata(
  { params }: Props,
  parent: ResolvingMetadata
): Promise<Metadata> {
  const { <%- dynamicParam %> } = await params;
  
  return {
    title: '', // TODO: Add title based on params
    url: `/<%- routePath %>`,
  };
}

export default function <%- pascalCaseName %>Page({ params }: Props) {
  return <<%- pascalCaseName %> {...params} />;
}
